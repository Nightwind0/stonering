function foo()
{
	print ("HI!");
}

function add(var $a, var $b)
{
	return $a + $b;
}

function doodle(var $a, var $b)
{
	while ( $a < $b) $a = $a + 1;
}

function yoodle(var $a,var $b)
{
	while ($a < $b) $a = $a - 1;
}

function array(var @stuff)
{
	var $len = len(@stuff);
	
	return $len;	
}

function dooda(var @stuff, var $b)
{
	return $b + 2;
}
