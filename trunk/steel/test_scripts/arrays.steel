
function printarray(var @array)
{
	printarray_(@array);
	print("\n");
}


function printarray_(var @array)
{
	var $i;
	print("{");	
	for($i=0;$i<len(@array);$i++)
	{
		if( is_array (@array[$i] ) )
		{
			printarray_(@array[$i]);
		}
		else 
		{				
			print(@array[$i]);
			
		}

		if( $i != len(@array) -1 )
		{
			print(",");
		}


	}
	print("}");
}

function make_2d(var $size_a, var $size_b)
{
	var @array[$size_a];
	var $i;
	for(;$i<$size_a;$i = $i + 1)
	{
		var @temp[$size_b];
		@array[$i] = @temp;
	}

	return @array;	
}


