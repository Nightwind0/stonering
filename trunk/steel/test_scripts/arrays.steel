function make_2d(var $size_a, var $size_b)
{
	var @array[$size_a];
	var $i;
	for(;$i<$size_a;$i = $i + 1)
	{
		var @temp[$size_b];
		@array[$i] = @temp;
	}

	return @array;	
}


