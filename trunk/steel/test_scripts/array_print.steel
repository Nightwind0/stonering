function printarray(var @array)
{
	printarray_(@array);
	print("\n");
}


function printarray_(var @array)
{
	var $i;
	print("\{");	
	for($i=0;$i<len(@array);$i++)
	{
		if( is_array (@array[$i] ) )
		{
			printarray_(@array[$i]);
		}
		else 
		{				
			print(@array[$i]);
			
		}

		if( $i != len(@array) -1 )
		{
			print(",");
		}


	}
	print("}");
}