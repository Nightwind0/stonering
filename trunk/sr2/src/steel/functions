function foo()
{
	print ("HI!");
}

function add($a, $b)
{
	return $a + $b;
}

function doodle($a, $b)
{
	while ( $a < $b) $a = $a + 1;
}

function yoodle($a, $b)
{
	while ($a < $b) $a = $a - 1;
}

function array(@stuff)
{
	var $len = @stuff;
	
	return @stuff;	
}

function dooda(@stuff, $b)
{
	return $b + 2;
}
