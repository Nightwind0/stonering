<statusEffectList>
            <statusEffect last="round_count" roundCount="8"  name="Paralyzed">
            <spriteRef ref="StatusEffect/Tangled"/>
            <onInvoke>
                <script id="SE_TangledInv">
                    <![CDATA[
                setCharacterToggle($_Character,$_CAN_FIGHT,false);
                setCharacterToggle($_Character,$_DRAW_PARALYZED,true);
                    ]]>
                </script>
            </onInvoke>
            <onRemove>
                <script id="SE_TangledRem">
                setCharacterToggle($_Character,$_CAN_FIGHT,true);
                setCharacterToggle($_Character,$_DRAW_PARALYZED,false);
                </script>
            </onRemove>
        </statusEffect>
        <statusEffect last="permanent"  name="Poison">
            <spriteRef ref="StatusEffect/Poisoned"/>
            <onInvoke>
                <script id="SE_PoisonInv">
                setCharacterToggle($_Character,$_DRAW_ILL,true);            
                </script>
            </onInvoke>
            <onRemove>
                <script id="SE_PoisonRem">
                setCharacterToggle($_Character,$_DRAW_ILL,false);
                </script>
            </onRemove>
                <onRound>
                        <script id="SE_Poison">
                            <![CDATA[
                            const $kMaxHp = getCharacterAttribute($_Character,$_MAXHP);
                            declare $damage = normal_random($kMaxHp * 0.025, $kMaxHp * 0.01); 
                            if($damage <1) $damage = 1;
                            doDamage($_Character,$damage);
                            if(inBattle()){
                                battle::doCharacterAnimation($_Character,getAnimation("recoil"));
                                battle::createDisplay($damage,$_Character,$_DISP_DAMAGE);
                            }else{
                            // TODO: Play sound bzzrp
                            }
                                ]]>
                        </script>
                </onRound>
        </statusEffect>
        <statusEffect last="round_count" roundCount="12" name="Doom">
            <spriteRef ref="StatusEffect/Doomed"/>
                <onRound>
                        <script id="SE_Doom">
                            <![CDATA[
                                if($_EffectRound > 5)
                                {
                                    kill($_Character);
                                }
                                ]]>
                        </script>
                </onRound>
        </statusEffect>
        <statusEffect last="round_count" roundCount="5" name="Tangled">
            <attributeModifier attribute="can_fight" type="toggle" value="false"/>
            <attributeModifier attribute="draw_stone" type="toggle" value="true"/>
            <attributeModifier attribute="draw_still" type="toggle" value="true"/>
            <spriteRef ref="StatusEffect/Tangled"/>
        </statusEffect>
        <statusEffect last="permanent" name="Stone">
            <attributeModifier attribute="can_act" type="toggle" value="false"/>
            <attributeModifier attribute="draw_stone" type="toggle" value="true"/>
            <spriteRef ref="StatusEffect/Stone"/>
                <onInvoke>
                        <script id="SE_Stone">
                        </script>
                </onInvoke>
        </statusEffect>
        <statusEffect last="round_count" roundCount="8" name="Dragon Rage">
            <spriteRef ref="StatusEffect/DragonRage"/>
            <attributeModifier attribute="str" type="multiply" value="1.25"/>
            <attributeModifier attribute="def" type="multiply" value="0.75"/>	    
                <onInvoke>
                        <script id = "SE_Enraged">
                        </script>
                </onInvoke>
        </statusEffect>
        <statusEffect last="round_count" roundCount="8" name="Focused">
            <spriteRef ref="StatusEffect/Focused"/>
            <attributeModifier attribute="dex" type="multiply" value="1.25"/>
                <onInvoke>
                        <script id="SE_Focused">
                        </script>
                </onInvoke>
        </statusEffect>
                
</statusEffectList>
